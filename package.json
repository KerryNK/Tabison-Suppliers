{
  "name": "tabison-suppliers-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Tabison Suppliers - Military, Safety & Professional Footwear Platform",
  "keywords": [
    "suppliers",
    "military",
    "safety",
    "footwear",
    "boots",
    "kenya",
    "e-commerce"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/KerryNK/Tabison-Suppliers.git"
  },
  "license": "MIT",
  "author": "Tabison Suppliers Team",
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "clean": "rm -rf node_modules backend/node_modules frontend/node_modules",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "start": "cd backend && npm start",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test"
  },
  "dependencies": {
    "@emotion/react": "latest",
